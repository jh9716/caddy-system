datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Status {
  AVAILABLE   // 가용
  LEAVE       // 휴무신청
  NOTICE      // 공지
  ADMIN       // 관리자
  MEMBER      // 조원
}

enum Team {
  TEAM_1
  TEAM_2
  TEAM_3
  TEAM_4
  TEAM_5
  TEAM_6
  TEAM_7
  TEAM_8
}

model Assignment {
  id        Int      @id @default(autoincrement())
  caddyName String
  team      Team
  status    Status
  notes     String?   // 특이사항(메모)
  createdAt DateTime @default(now())
}
