generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Assignment {
  id        Int      @id @default(autoincrement())
  caddyName String
  team      Team
  status    Status
  notes     String?
  createdAt DateTime @default(now())
}

enum Status {
  AVAILABLE
  LEAVE
  NOTICE
  ADMIN
  MEMBER
}

enum Team {
  TEAM_1
  TEAM_2
  TEAM_3
  TEAM_4
  TEAM_5
  TEAM_6
  TEAM_7
  TEAM_8
}
